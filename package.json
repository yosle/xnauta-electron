{
  "name": "xnauta",
  "productName": "xnauta",
  "version": "0.0.3",
  "description": "Cliente Multiplataforma para el Portal de ETECSA",
  "main": ".webpack/main",
  "repository": {
    "url": "https://github.com/yosle/xnauta-electron",
    "type": "public"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package executableName=xnauta",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Yosleivy Baez Acosta",
    "email": "yosle007@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@electron-forge/plugin-webpack": "^6.1.1",
    "@electron-forge/publisher-github": "^7.4.0",
    "@types/cheerio": "^0.22.31",
    "@types/electron-store": "^3.2.0",
    "@types/minimist": "^1.2.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "css-loader": "^6.0.0",
    "electron": "24.0.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@sentry/electron": "^5.0.0",
    "@types/tough-cookie": "^4.0.2",
    "cheerio": "^1.0.0-rc.12",
    "data-store": "^4.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "form-data": "^4.0.0",
    "got": "11.8.2",
    "minimist": "^1.2.8",
    "tough-cookie": "^4.1.2",
    "update-electron-app": "^3.0.0"
  },
  "plugins": [
    [
      "@electron-forge/plugin-webpack",
      {
        "mainConfig": "./webpack.main.config.js",
        "devContentSecurityPolicy": "connect-src 'self' * 'unsafe-eval'",
        "renderer": {
          "config": "./webpack.renderer.config.js",
          "entryPoints": [
            {
              "html": "./src/index.html",
              "js": "./src/renderer/renderer.ts",
              "name": "main_window"
            }
          ]
        }
      }
    ]
  ]
}
